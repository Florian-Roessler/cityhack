{
  "id": "3d-buildings",
  "source": "composite",
  "source-layer": "building",
  "filter": [
    "==",
    "extrude",
    "true"
  ],
  "type": "fill-extrusion",
  "minzoom": 6,
  "paint": {
    "fill-extrusion-color": "#aaa",
    "fill-extrusion-height": [
      "interpolate",
      [
        "linear"
      ],
      [
        "zoom"
      ],
      6,
      0,
      6.05,
      [
        "get",
        "height"
      ]
    ],
    "fill-extrusion-base": [
      "interpolate",
      [
        "linear"
      ],
      [
        "zoom"
      ],
      6,
      0,
      6.05,
      [
        "get",
        "min_height"
      ]
    ],
    "fill-extrusion-opacity": 0.6
  }
}
